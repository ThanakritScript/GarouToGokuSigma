local ReplicatedStorage = game:GetService("ReplicatedStorage")
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local targetPart = character:WaitForChild("HumanoidRootPart")

local particle = ReplicatedStorage:WaitForChild("Resources"):WaitForChild("WhirlwindDrop"):WaitForChild("GroundImpact"):WaitForChild("DownLines")
local ermskibidiParticles = particle:GetChildren()

for _, child in ipairs(ermskibidiParticles) do
    if child:IsA("ParticleEmitter") and child.Name == "9" then
        child.Color = ColorSequence.new(Color3.fromRGB(200, 200, 255))
        child.Size = NumberSequence.new(1.3)
    end
end
task.wait(1.5)
local foundEmitters = false
for i = 1, 2 do
    for _, descendant in ipairs(particle:GetDescendants()) do
        if descendant:IsA("ParticleEmitter") then
            foundEmitters = true
            local clonedParticle = descendant:Clone()
            clonedParticle.Parent = targetPart
            clonedParticle:Emit(3)
        end
    end
    task.wait(0.01)
end

